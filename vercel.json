{
  "version": 2,
  "buildCommand": "cd src/hourly_analysis && python3 build.py && cd ../.. && python3 -m pip install -r requirements.txt",
  "outputDirectory": "build",
  "builds": [
    {
      "src": "src/web/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/hourly/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/hourly",
      "dest": "/build/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/web/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "github": {
    "silent": true
  }
}
