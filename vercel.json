{
    "version": 2,
    "builds": [
        {
            "src": "src/web/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/web/wsgi.py"
        }
    ],
    "functions": {
        "src/web/wsgi.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "env": {
        "FLASK_ENV": "production"
    }
}
